<!DOCTYPE html>
<html>
<head>
  <title>Snowforest</title>
  <%= stylesheet_link_tag    'application', media: "all", "data-turbolinks-track" => true %>
  <%= stylesheet_link_tag    './vendor/assets/stylesheets/css/style', media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

    <div class="container">
        <% if flash[:notice] %>
            <div id="notice"><%= flash[:notice] %></div>
        <% end %>

        <%= yield %>
    </div>



</body>
</html>
